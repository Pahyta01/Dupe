-- üîê –ö–ª—é—á–µ–≤–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞
if not getgenv or getgenv().PahTeTKey ~= "PahTeT-Access-Core-918X" then
	warn("‚ùå Invalid or missing key")
	return
end

local player = game:GetService("Players").LocalPlayer
local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.Name = "PahTeTLauncher"
gui.ResetOnSpawn = false

-- üñº GUI Frame
local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 400, 0, 360)
frame.Position = UDim2.new(0.5, -200, 0.5, -180)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.BorderSizePixel = 0

local stroke = Instance.new("UIStroke", frame)
stroke.Thickness = 2
stroke.Color = Color3.new(1, 1, 1)
stroke.Transparency = 0.5

-- üê± –ö–æ—Ç
local catImage = Instance.new("ImageButton", gui)
catImage.Size = UDim2.new(0, 160, 0, 160)
catImage.Position = UDim2.new(0.5, 275, 0.5, -80)
catImage.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
catImage.BorderSizePixel = 0
catImage.Image = "rbxassetid://109112430589154"

local catStroke = Instance.new("UIStroke", catImage)
catStroke.Thickness = 2
catStroke.Color = Color3.new(1, 1, 1)
catStroke.Transparency = 0.5

local sound = Instance.new("Sound", catImage)
sound.SoundId = "rbxassetid://8551016315"
sound.Volume = 1

local TweenService = game:GetService("TweenService")
catImage.MouseButton1Click:Connect(function()
	sound:Play()
	local grow = TweenService:Create(catImage, TweenInfo.new(0.15), {
		Size = UDim2.new(0, 180, 0, 180),
		Position = UDim2.new(0.5, 265, 0.5, -90)
	})
	local shrink = TweenService:Create(catImage, TweenInfo.new(0.15), {
		Size = UDim2.new(0, 160, 0, 160),
		Position = UDim2.new(0.5, 275, 0.5, -80)
	})
	grow:Play()
	grow.Completed:Connect(function()
		shrink:Play()
	end)
end)

-- üñ± –ü–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏–µ
local dragging, dragInput, dragStart, startPos
frame.Active = true
frame.Draggable = false

frame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
		dragStart = input.Position
		startPos = frame.Position
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

frame.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement then
		dragInput = input
	end
end)

game:GetService("UserInputService").InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		local delta = input.Position - dragStart
		local newPos = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		frame.Position = newPos
		catImage.Position = UDim2.new(newPos.X.Scale, newPos.X.Offset + 475, newPos.Y.Scale, newPos.Y.Offset + 100)
	end
end)

-- ‚ùå –ó–∞–∫—Ä—ã—Ç—å
local closeBtn = Instance.new("TextButton", frame)
closeBtn.Size = UDim2.new(0, 30, 0, 30)
closeBtn.Position = UDim2.new(1, -35, 0, 5)
closeBtn.Text = "X"
closeBtn.BackgroundColor3 = Color3.fromRGB(100, 0, 0)
closeBtn.TextColor3 = Color3.new(1, 1, 1)
closeBtn.Font = Enum.Font.SourceSansBold
closeBtn.TextSize = 18
closeBtn.MouseButton1Click:Connect(function()
	gui:Destroy()
end)

-- üè∑ –ó–∞–≥–æ–ª–æ–≤–æ–∫
local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1, -40, 0, 40)
title.Position = UDim2.new(0, 20, 0, 10)
title.Text = "PahTeT Hub"
title.TextColor3 = Color3.new(1, 1, 1)
title.BackgroundTransparency = 1
title.Font = Enum.Font.SourceSansBold
title.TextSize = 24
title.TextXAlignment = Enum.TextXAlignment.Left

-- üîÅ Dupe
local dupeBtn = Instance.new("TextButton", frame)
dupeBtn.Size = UDim2.new(0.8, 0, 0, 40)
dupeBtn.Position = UDim2.new(0.1, 0, 0.25, 0)
dupeBtn.Text = "Dupe"
dupeBtn.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
dupeBtn.TextColor3 = Color3.new(1, 1, 1)
dupeBtn.Font = Enum.Font.SourceSansBold
dupeBtn.TextSize = 20
dupeBtn.MouseButton1Click:Connect(function()
	local rs = game:GetService("ReplicatedStorage")
	if rs:FindFirstChild("Remotes") and rs.Remotes:FindFirstChild("Trap") then
		rs.Remotes.Trap:FireServer(nil, "\195\191")
		warn("Dupe triggered")
	else
		warn("Remote not found")
	end
end)

-- üîÑ Rejoin
local rejoinBtn = Instance.new("TextButton", frame)
rejoinBtn.Size = UDim2.new(0.8, 0, 0, 40)
rejoinBtn.Position = UDim2.new(0.1, 0, 0.45, 0)
rejoinBtn.Text = "Rejoin"
rejoinBtn.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
rejoinBtn.TextColor3 = Color3.new(1, 1, 1)
rejoinBtn.Font = Enum.Font.SourceSansBold
rejoinBtn.TextSize = 20
rejoinBtn.MouseButton1Click:Connect(function()
	game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId, player)
end)

lirpBtn.MouseButton1Click:Connect(function()
	local MessageBox = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/NotificationGUI/main/source.lua"))()
	local box = MessageBox.Show({
		Position = UDim2.new(0.5, 0, 0.5, 0),
		Text = "Start Lirp?",
		Description = "–¢—ã —Ç–æ—á–Ω–æ —Ö–æ—á–µ—à—å –∑–∞–ø—É—Å—Ç–∏—Ç—å Lirp?\n–°–∫—Ä–∏–ø—Ç –æ—Ç: LirpOfficial",
		MessageBoxIcon = "Warning",
		MessageBoxButtons = "YesNo",
		Result = function(res)
			if res == "Yes" then
				if box and box:FindFirstChild("Main") then
					box:Destroy()
				end
				gui:Destroy()
				getgenv().Key = "EnableModDetectorAgain@!%!5#*@y*UhUEasgfh*di2%#u%@#i#*%@o%12O5HEA*SLGFSTIUDGao@!%@!*(^#T(@UW*UGJIOHSEDH@(#@"
				loadstring(game:HttpGet("https://raw.githubusercontent.com/LirpOfficial/Lirp/refs/heads/main/Loader", true))()
			else
				warn("Lirp launch cancelled.")
			end
		end
	})
end)

